  "version": "0.2.0",
  "configurations": [
    {
      "name": "PowerShell: Launch Icarus",
      "type": "PowerShell",
      "request": "launch",
      "script": "${workspaceFolder}\\main.ps1",
      "args": ["-ServerCfg \"Icarus\""],
      "cwd": "${workspaceFolder}"
    },
    {
      "name": "PowerShell: Launch Icarus Update Check",
      "type": "PowerShell",
      "request": "launch",
      "script": "${workspaceFolder}\\main.ps1",
      "args": ["-ServerCfg \"Icarus\" -UpdateCheck"],
      "cwd": "${workspaceFolder}"
    }
  ]
}
