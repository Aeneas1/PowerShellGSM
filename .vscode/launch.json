    "version": "0.2.0",
    "configurations": [
        {
            "name": "PowerShell: Launch Satisfactory",
            "type": "PowerShell",
            "request": "launch",
            "script": "${workspaceFolder}\\main.ps1",
            "args": ["-ServerCfg \"Satisfactory\""],
            "cwd": "${workspaceFolder}"
        },
        {
            "name": "PowerShell: Launch Satisfactory Update Check",
            "type": "PowerShell",
            "request": "launch",
            "script": "${workspaceFolder}\\main.ps1",
            "args": ["-ServerCfg \"Satisfactory\" -UpdateCheck"],
            "cwd": "${workspaceFolder}"
        }
    ]
}